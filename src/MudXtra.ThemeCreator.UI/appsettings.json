{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "postgresql": "Server=[dbName];Port=[dbPort];Database=Themes;Username=[dbUser];Password=[dbPassword]",
        "vs_sql_server": "Server=(LocalDB)\\MSSQLLocalDB;Database=ThemeCreator;Trusted_Connection=True;"
    },
    "Auth0": {
        "Domain": "{DOMAIN}",
        "ClientId": "{CLIENT_ID}"
    },
    "domain": "auth0.com",
    "dbUser": "user",
    "dbPort": "1234",
    "dbPassword": "password",
    "dbName": "db.example.com",
    "clientid": "clientid"
}
