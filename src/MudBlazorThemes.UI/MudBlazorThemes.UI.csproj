    <UserSecretsId>14c27eb2-8fb1-4121-a349-9d55e3b88f6f</UserSecretsId>
    <ScopedCssEnabled>true</ScopedCssEnabled>      
    <ApplicationIcon>wwwroot\ThemeCreator.ico</ApplicationIcon>      
