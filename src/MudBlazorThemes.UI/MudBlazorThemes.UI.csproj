  <ItemGroup>
    <ProjectReference Include="..\MudBlazorThemes.DAL\MudBlazorThemes.DAL.csproj" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Auth0.AspNetCore.Authentication" Version="1.4.1" />
